{
    "build": [{
        "src": "django_ledger_project/settings.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "django_ledger_project/wsgi.py"
        }
    ]
}
